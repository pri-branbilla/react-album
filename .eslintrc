{
  "parser": "babel-eslint",
  "env": {
    "jest": true,
    "browser": true
  },
  "plugins": [
    "react", "modules", "import", "react-native"
  ],
  "rules": {
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "react-native/no-inline-styles": 2,
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true
      }
    ],
    "max-len": [
      "error",
      { "code": 80 }
    ],
    "comma-dangle": [
      "error",
      "always-multiline"
    ]
  }
}
